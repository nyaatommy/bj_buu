}
sub _pet_status_up {
	my($k,$v) = @_;
	$m{$k} += $v;
	$m{$k . '_org'} += $v;
	$mes.="$pets[$m{pet}][1]★$m{pet_c}から力をあたえられ、$m{name}の$e2j{$k}が$v上がった<br>";
}
sub _pet_weapon {
	my @borns = @_;
	my $v = $borns[int(rand(@borns))];
	$mes.="$pets[$m{pet}][1]★$m{pet_c}は$weas[$v][1]に形を変えた!<br>$weas[$v][1]は預かり所に送られました<br>";
	&send_item($m{name}, 1, $v, 0, $m{pet_c}, 1);
}
sub _pet_eggman {
	my @borns = @_;
	my $v = $borns[int(rand(@borns))];
	$mes .= "$pets[$m{pet}][1]★$m{pet_c}は全ての力を解き放った!!<br>なんと…$pets[$m{pet}][1]★$m{pet_c}は";
	if ($v eq '0') {
		$mes.="そのまま消滅してしまった…<br>";
	}
	else {
		$mes.="$eggs[$v][1]に形を変えた!<br>$eggs[$v][1]は預かり所に送られました<br>";
		&send_item($m{name}, 2, $v, $m{pet_c} * 50, 0, 1);
	}
}
sub _pet_sedai_job {
	my @nos = @_;
	my $no = $nos[0];
	if ($m{pet_c} > rand(20)) {
		$no = $nos[1];
	}
	$m{job} = $no;
	$mes .= "$pets[$m{pet}][1]★$m{pet_c}の力で $m{name} の職業が $jobs[$no][1]になりました<br>";
	$mes .= "役目を終えた $pets[$m{pet}][1]★$m{pet_c} は光の彼方へ消えていった…<br>";
	&remove_pet;
}

sub _pet_steal_country {
	my($k,$v) = &_steal_country(@_);
	&mes_and_world_news("<b>$pets[$m{pet}][1]★$m{pet_c}を使い$cs{name}[$y{country}]の$e2j{$k}を $v 奪いました</b>");
	&write_world_news("$c_mの$m{name}が$pets[$m{pet}][1]★$m{pet_c}を使ったことによる罪で$cs{name}[$y{country}]の$cs{prison_name}[$y{country}]に幽閉されました");
	&add_prisoner;
}

sub _steal_country {
	my($k,$v) = @_;

	my $max_value = $cs{$k}[1];
	$y{country} = 1;
	for my $i (2 .. $w{country}) {
		if ($cs{$k}[$i] > $max_value) {
			$y{country} = $i;
			$max_value = $cs{$k}[$i];
		}
	}

	$v = $cs{$k}[$y{country}] if $v > $cs{$k}[$y{country}];
	$cs{$k}[$y{country}] -= $v;
	$cs{$k}[$m{country}] += $v unless $y{country} eq $m{country};
	&write_cs;

	return ($k, $v);
}

sub _pet_add_prisoner {
	my @ks = (qw/war dom pro mil ceo/);
	my $k = $ks[int(rand(@ks))];

	for my $i (1 .. $w{country}) {
		next if $cs{$k}[$i] eq '';
		next if $i eq $m{country};
		
		&regist_you_data($cs{$k}[$i], 'lib', 'prison');
		&regist_you_data($cs{$k}[$i], 'tp', 100);
		&regist_you_data($cs{$k}[$i], 'y_country',  $m{country});
		&regist_you_data($cs{$k}[$i], 'wt', $GWT * 60);
		&regist_you_data($cs{$k}[$i], 'act', 0);
		
		open my $fh, ">> $logdir/$m{country}/prisoner.cgi" or &error("$logdir/$m{country}/prisoner.cgi が開けません");
		print $fh "$cs{$k}[$i]<>$i<>\n";
		close $fh;
	}
	&mes_and_world_news("<b>$pets[$m{pet}][1]★$m{pet_c}を使い各国の $e2j{$k} が$c_mの$cs{prison_name}[$m{country}]に幽閉されました</b>");
	
	$y{country} = $m{country};
	&add_prisoner;
}

sub _pet_random {
	require "$datadir/parupunte.cgi";
	if ($w{world} ne '1') {
		&{$effects[int(rand(@effects))]};
	} else {
		&{$effects[2]};
	}
}

