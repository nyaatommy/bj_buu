sub use_pet {
	my($k, $v) = @_;
	if ( $pets[$m{pet}][2] eq $k ) {
		my($return_value, $is_return) = $pets[$m{pet}][3]->($v);
		if ($m{pet} < 0) {
			$m{pet_c}--;
			if ($m{pet_c} <= 0) {
				$m{pet} = 0;
				$m{pet_c} = 0;
			}
		}
		return $is_return ? int($return_value) : $v;
	}
	return $v;
}
